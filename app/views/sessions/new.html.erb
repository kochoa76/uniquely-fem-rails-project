<h1>Log In</h1>
<%= link_to('Log in with Facebook!', '/auth/facebook') %>
<br></br>
<%= link_to "Sign Up", new_user_path %>
<br></br>

<%=form_tag({url: '/signin', method: :post}) do %>
    <%= label_tag :username %>
    <%= text_field_tag :username, nil, :required => true %>
    <br></br>
    <%= label_tag :email %>
    <%= text_field_tag :email, nil, :required => true %>
    <br></br>
    <%= label_tag :password %>
    <%= password_field_tag :password, nil, :required => true  %>
    <br></br>
    <%= submit_tag "Log In" %>
    <% end %>



